    "class Solution:\n",
    "    def tribonacci(self, n: int) -> int:\n",
    "        T = [0,1,1]\n",
    "        if n < 3:\n",
    "            return T[n]\n",
    "        for i in range(n-2):\n",
    "            T = [T[1], T[2], T[0] + T[1] + T[2]]\n",
    "        return T[-1]"
   ]
